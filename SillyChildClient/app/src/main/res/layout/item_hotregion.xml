<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_region"
    android:layout_width="@dimen/dimen118"
    android:layout_height="@dimen/dimen89">

    <ImageView
        android:id="@+id/img_region"
        android:layout_width="@dimen/dimen118"
        android:layout_height="@dimen/dimen89"
        android:scaleType="fitXY"
        android:src="@mipmap/placeholderfigure" />


    <TextView
        android:id="@+id/tv_region"
        style="@style/MatchMatch"
        android:background="@mipmap/hp_mask"
        android:gravity="center"
        android:text="日本"
        android:textColor="@color/whiteColors"
        android:textSize="@dimen/textsize15" />


</RelativeLayout>